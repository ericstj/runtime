  <ItemGroup Condition="'$(TestWindowsBase)' == '.NETFramework'">
    <Compile Include="$(MSBuildThisFileDirectory)Tests.NetFramework.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.IO.Packaging.TestData" Version="$(SystemIOPackagingTestDataVersion)" />
  </ItemGroup>
